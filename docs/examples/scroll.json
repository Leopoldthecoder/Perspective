{
  "stages": [{
    "id": "universe",
    "scrollNumber": 60,
    "transition": 0,
    "easing": "linear",
    "items": [{
      "id": "background",
      "effects": [{
        "property": "transform",
        "start": "translateY(0px)",
        "end": "translateY(-100px)"
      }]
    }, {
      "id": "satellite",
      "effects": [{
        "property": "transform",
        "start": "translateY(0px)",
        "end": "translateY(-180px)"
      }]
    }, {
      "id": "planet",
      "effects": [{
        "property": "transform",
        "start": "translateY(0px)",
        "end": "translateY(-700px)"
      }]
    }]
  }, {
    "id": "svg",
    "scrollNumber": 10,
    "transition": 200,
    "items": [{
      "id": "dash",
      "effects": [{
        "property": "strokeDasharray",
        "start": "0 126",
        "end": "126 0"
      }]
    }]
  }, {
    "id": "opacity",
    "transition": 2000,
    "items": [{
      "id": "opacity",
      "effects": [{
        "property": "opacity",
        "start": "0",
        "end": "0.9"
      }, {
        "property": "transform",
        "start": "translateY(0px)",
        "end": "translateY(-50px)"
      }]
    }]
  }, {
    "id": "color",
    "scrollNumber": 7,
    "transition": 500,
    "items": [{
      "id": "battery",
      "effects": [{
        "property": "width",
        "start": "100px",
        "end": "200px"
      }, {
        "property": "backgroundColor",
        "start": "hsla(150, 100%, 50%, 1)",
        "end": "hsla(50, 100%, 50%, 0.3)"
      }]
    }, {
      "id": "text",
      "effects": [{
        "property": "color",
        "start": "hsl(50, 100%, 50%)",
        "end": "hsl(150, 100%, 50%)"
      }]
    }]
  }]
}